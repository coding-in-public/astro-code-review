---
import { twMerge } from "tailwind-merge";

import { Image } from "astro:assets";
import stars from "@/assets/stars.png";

type Props = {
  classes?: string;
};

const { classes } = Astro.props;
---

<Image
  src={stars}
  alt=""
  format="avif"
  class={twMerge("absolute -z-10 pointer-events-none", classes)}
/>
